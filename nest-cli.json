{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/my_nest_monorepo/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/my_nest_monorepo/tsconfig.app.json"
  },
  "generateOptions": {
    "spec": false,
    "flat": false
  },
  "monorepo": true,
  "root": "apps/my_nest_monorepo",
  "projects": {
    "my_nest_monorepo": {
      "type": "application",
      "root": "apps/my_nest_monorepo",
      "entryFile": "main",
      "sourceRoot": "apps/my_nest_monorepo/src",
      "compilerOptions": {
        "tsConfigPath": "apps/my_nest_monorepo/tsconfig.app.json"
      }
    },
    "my-app": {
      "type": "application",
      "root": "apps/my-app",
      "entryFile": "main",
      "sourceRoot": "apps/my-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/my-app/tsconfig.app.json"
      }
    },
    "all-provider": {
      "type": "application",
      "root": "apps/all-provider",
      "entryFile": "main",
      "sourceRoot": "apps/all-provider/src",
      "compilerOptions": {
        "tsConfigPath": "apps/all-provider/tsconfig.app.json"
      }
    },
    "all-decorator": {
      "type": "application",
      "root": "apps/all-decorator",
      "entryFile": "main",
      "sourceRoot": "apps/all-decorator/src",
      "compilerOptions": {
        "tsConfigPath": "apps/all-decorator/tsconfig.app.json"
      }
    },
    "context": {
      "type": "application",
      "root": "apps/context",
      "entryFile": "main",
      "sourceRoot": "apps/context/src",
      "compilerOptions": {
        "tsConfigPath": "apps/context/tsconfig.app.json"
      }
    },
    "what-middleware": {
      "type": "application",
      "root": "apps/what-middleware",
      "entryFile": "main",
      "sourceRoot": "apps/what-middleware/src",
      "compilerOptions": {
        "tsConfigPath": "apps/what-middleware/tsconfig.app.json"
      }
    },
    "all-pipe": {
      "type": "application",
      "root": "apps/all-pipe",
      "entryFile": "main",
      "sourceRoot": "apps/all-pipe/src",
      "compilerOptions": {
        "tsConfigPath": "apps/all-pipe/tsconfig.app.json"
      }
    },
    "all-interceptor": {
      "type": "application",
      "root": "apps/all-interceptor",
      "entryFile": "main",
      "sourceRoot": "apps/all-interceptor/src",
      "compilerOptions": {
        "tsConfigPath": "apps/all-interceptor/tsconfig.app.json"
      }
    },
    "all-exception": {
      "type": "application",
      "root": "apps/all-exception",
      "entryFile": "main",
      "sourceRoot": "apps/all-exception/src",
      "compilerOptions": {
        "tsConfigPath": "apps/all-exception/tsconfig.app.json"
      }
    },
    "multiple-version": {
      "type": "application",
      "root": "apps/multiple-version",
      "entryFile": "main",
      "sourceRoot": "apps/multiple-version/src",
      "compilerOptions": {
        "tsConfigPath": "apps/multiple-version/tsconfig.app.json"
      }
    }
  }
}